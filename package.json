{
  "name": "game-of-life",
  "version": "1.0.0",
  "author": "SKCrawford",
  "description": "Conway's Game of Life Simulator",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prestart": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "start:log": "yarn start > all.log 2>&1",
    "start:dev": "ts-node src/index.ts",
    "test": "jest"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "express": "^4.17.1",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/socket.io": "^2.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslib": "^1.10.0",
    "typescript": "^3.6.2"
  },
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "modulePathIgnorePatterns": [
      ".old$"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "keywords": [
    "conway's",
    "game",
    "of",
    "life",
    "conway"
  ],
  "homepage": "https://github.com/SKCrawford/game-of-life-express#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SKCrawford/game-of-life-express.git"
  },
  "bugs": {
    "url": "https://github.com/SKCrawford/game-of-life-express/issues"
  }
}
